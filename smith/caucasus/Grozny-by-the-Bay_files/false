<script type="text/javascript">

jQuery(function($) {
	if(window.location.hash){
		var hsh = window.location.hash.substring(1);
		$('#viewcomments').hide();
		$('#hidecomments').show();
		$('#oldcomments').show();	
		window.location.hash = hsh;
	}
});

jQuery(function($) {
	$('#viewcomments').click(function(){
		$('#viewcomments').hide();
		$('#hidecomments').show();
		$('#oldcomments').show();			
	});	
	$('#hidecomments').click(function(){
		$('#viewcomments').show();
		$('#hidecomments').hide();	
		$('#oldcomments').hide();

	});	

});
</script>


