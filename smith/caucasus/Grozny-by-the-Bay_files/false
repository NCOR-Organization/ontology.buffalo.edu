<script language='javascript'>
// copy cookies to local storage as nec.
localStorageToCookie = function(name, expires, reload)
{
  try
  {
  if( localStorage.getItem(name) )
  {
    var expiryString = "";
    if( expires != 0 )
    {
      var expiryTime = new Date();
      expiryTime.setTime(expires*1000);
      expiryString = "expires=" + expiryTime.toGMTString() + ";";
    }
    
    // get a good value.
    //var value = unescape(localStorage.getItem(name)); ! don't do this.  Do this:
    var value = localStorage.getItem(name);
    value = value.replace(/\|\|\*\*\|\|/g, "'");
    
    if( value == "DELETE"
    &&  name != "pjsso"  )
    {
      value = "";
      expiryString = "expires=Thu, 01 Jan 1970 00:00:01 GMT;";
    }
    
    if( name == "pjsso-cache"  )
    {
      value = escape(value);
    }
    
    document.cookie = name + "=" + value + "; "+expiryString+" path=/;"; 
    localStorage.setItem(name, "");       
    
    if( reload )
    {
      top.location.reload(true);
    }
  }
  }catch(e){}
}

try
{
var expires = localStorage.getItem("pjsso-expires");
if( !expires || expires == "DELETE")
{
  expires = "0";
}

localStorageToCookie("pjsso-refresh", expires, false);
localStorageToCookie("pjsso-cache", expires, false);
localStorageToCookie("pjsso-expires", expires, false);
localStorageToCookie("pjsso-pending", expires, false);
localStorageToCookie("pjsso-admin", expires, false);
localStorageToCookie("pjsso-no-cache", expires, false);
localStorageToCookie("pjsso", expires, true);
}catch(e){}
</script>

